<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:camel="http://camel.apache.org/schema/spring"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xsi:schemaLocation=" http://www.springframework.org/schema/beans 
    					http://www.springframework.org/schema/beans/spring-beans.xsd  
						http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">
						
	
    <!-- <bean id="executeMatchUpdateConverter" class="org.mycompany.entities.converter.ExecuteMatchUpdateConverter"/> -->
    <camelContext id="camel" xmlns="http://camel.apache.org/schema/spring">
        <propertyPlaceholder id="properties" location="route.properties"/>
		 <dataFormats>
         	<jaxb id="jaxb" prettyPrint="true" objectFactory="true" contextPath="org.mycompany.entities"/>
        </dataFormats>
        <route>        	
			<from uri="amqp:queue:patient"/>
			<log id="_log_unmarshal1" message="First Message tin tin--> ${body}"/>
			<unmarshal  ref="jaxb"/>
			<log id="_log_unmarshal2" message="after Unmarshalling Message --> ${body}"/>
			<to uri="direct:route-matchupdate"/> 
		</route>
		<route>
        	<from id="route-matchupdate" uri="direct:route-matchupdate"/>
        	<convertBodyTo type="com.nextgate.ws.entities.ExecuteMatchUpdate"/>
        	<!-- <to uri="amqp:queue:patientnew"/>-->
			<log id="_log_unmarshal" message="${body}"/>
			
			<marshal>
				<jaxb id="jaxb_1" prettyPrint="true" objectFactory="true" contextPath="com.nextgate.ws.entities"/>
			</marshal>
			<log id="_log_marshal_route3" message="${body}"/>
			
		</route>
    </camelContext>
</beans>
